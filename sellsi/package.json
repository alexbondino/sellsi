{
  "name": "sellsi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "build:analyze": "vite build --config ./analyze-bundle.js",
    "build:cdn": "vite build --config ./vite.config.cdn.js",
    "build:cdn-analyze": "vite build --config ./analyze-bundle.js && vite build --config ./vite.config.cdn.js",
    "preview": "vite build --mode staging && vite preview --mode staging",
    "preview:staging": "vite build --mode staging && vite preview --mode staging",
    "preview:production": "vite build --mode production && vite preview --mode production",
    "lint": "eslint .",
    "optimize": "npm run lint && npm run build",
    "optimize:cdn": "npm run lint && npm run build:cdn",
    "analyze:cycles": "node ../detect_cycles.js --format pretty --suggest-breaks",
    "analyze:cycles:md": "node ../detect_cycles.js --format md --suggest-breaks --output ./Documentacion/cycles/cycle-report.md",
    "size-check": "npm run build && du -sh dist/*",
    "flags:generate": "node scripts/generate-flag-subset.cjs",
    "bundle:collect": "node scripts/bundle/compare-chunks.mjs collect --label",
    "bundle:diff": "node scripts/bundle/compare-chunks.mjs diff",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=\"unit\" --coverage --coverageDirectory=coverage/unit",
    "test:integration": "jest --testPathPatterns=\"integration\" --coverage --coverageDirectory=coverage/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:memory": "playwright test --config=playwright.memory.config.ts tests/memory/memory-leak.spec.ts",
    "test:performance": "playwright test --config=playwright.memory.config.ts tests/memory/performance.spec.ts",
    "test:network": "playwright test --config=playwright.memory.config.ts tests/memory/network.spec.ts",
    "test:api": "playwright test --config=playwright.memory.config.ts tests/memory/api-efficiency.spec.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pipeline": "node scripts/test-pipeline.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run build && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@mui/x-charts": "^8.5.0",
    "@react-pdf/renderer": "^4.3.0",
    "@sentry/react": "^8.13.0",
    "@sentry/vite-plugin": "^3.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "flag-icons": "^7.2.3",
    "framer-motion": "^12.16.0",
    "glob": "^11.0.3",
    "immer": "^10.1.1",
    "lodash.debounce": "^4.0.8",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "react-phone-input-2": "^2.15.1",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.6.0",
    "react-toastify": "^11.0.5",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "react-zoom-pan-pinch": "^3.7.0",
    "speakeasy": "^2.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@emotion/server": "^11.11.0",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.55.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.2.2",
    "babel-jest": "^30.1.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "buffer": "^6.0.3",
    "critical": "^5.1.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  }
}
